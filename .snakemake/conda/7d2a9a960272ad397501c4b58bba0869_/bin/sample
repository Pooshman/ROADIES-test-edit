#!/home/AD.UCSD.EDU/ttl074/wga/wga-phylo/.snakemake/conda/7d2a9a960272ad397501c4b58bba0869_/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'mmtf-python==1.0.2','console_scripts','sample'
__requires__ = 'mmtf-python==1.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('mmtf-python==1.0.2', 'console_scripts', 'sample')()
    )
